		},
		function() {
			$("#album_intro").addClass("intro_less");
			$(this).attr('id','show_more').html("详细");
		}
	);
});
	//提交音频
	$('#submit_music').hover(function(){
		$(this).find('.show_tips').show();
	},function(){
		$(this).find('.show_tips').hide();
	});
$('#more_btn').bind("click", function(){
