function libSongDel(id,obj){
	if(id.match(/\d+/)==null) return false;
	$(obj).hide().after('<img id="loading2" style="margin-left:140px;" height="16" width="16" alt=""  src="http://img.xiami.net/res/img/default/loading.gif"/>');
