}

$(document).ready(function(){
	if (comefrom == 'sogou' || comefrom == 'taobao'){
		showDialog('/app/sogou/guide/id/'+song_id+'?'+Math.random());
	}
	$('#song_acts li').mouseenter(function(){
		$(this).addClass('acts_hover');
	})
	.mouseleave(function(){
		$(this).removeClass('acts_hover');
	});
	
	$(".album_intro_toggle a").toggle(
		function() {
			$("#lrc").addClass("less_lrc");
			$(this).attr('id','show_more').html("详细");
		},
		function() {
			$("#lrc").removeClass("less_lrc");
			$(this).attr('id','show_less').html("收缩");
		}
	);
	
	$.getJSON('/count/getplaycount',{'id':song_id,'type':'song', '_xiamitoken':_xiamitoken},function(data){
		$('#play_count_num').html(data.plays);
	});
});
$('#more_btn').bind("click", function(){
	if (!$(this).hasClass("song_less")) {
		$(this).html('- 更少').addClass("song_less");
		$('.share_btns').hide();
		$('#full_share').show();							
	} else {
		$(this).html('+ 更多').removeClass("song_less");
		$('#full_share').hide();
		$('.share_btns').show();
	}				
});

)} 

