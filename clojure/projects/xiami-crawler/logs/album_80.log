		$('.grade span').text($(this).text()); 
	}).click(function(){
		$(target).removeClass('hover').removeClass('selected');
		var i=parseInt($(target).index($(this)[0])); 
		for (j=0; j<=i; j++){$($(target).get(j)).addClass('selected');}
		$('#album_cover .grade_v').val(i+1);
