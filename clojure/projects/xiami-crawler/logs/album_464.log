		var self=this;
		XN_RequireFeatures(["Connect"], function(){
			XN.Main.init(self.ApiKey,self.Receiver);
			XN.Connect.get_status().waitUntilReady(function (login_state) {
				if (login_state != XN.ConnectState.connected) self.Init();
