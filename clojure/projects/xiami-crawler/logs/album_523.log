 {:tag :script, :attrs nil, :content (
var commentType = 1;
var object_id = parseInt(523);
var uid = parseInt('') || 0;
var content_txt = '';
var content_html = '';
var topic = '';
var relids = '';

function regReportEvent() {
    $('.post_item').each(function(i){
        var self = this;
        $(self).unbind('mouseenter.report mouseleave.report');
        $(self).bind('mouseenter.report',function(){
            temp = setTimeout(function(){
                $(self).children('.info').children('.report').children('.reportbox').show();
            }, 200);
        }).bind('mouseleave.report', function(){
            clearTimeout(temp);
            $(self).children('.info').children('.report').children('.reportbox').hide();
        });
    });
}
regReportEvent();

$(document).ready(function(){
	var $commentDiv 	= $("#wall_list"),
	$commentForm 	= $('#post_form'),
	$commentCount	= $('.wall_list_count').find('span');
	$btn_Submit 	= $commentForm.find('.bt_sub4w'),
	$textarea 		= $commentForm.find('textarea'),
	$count 			= $commentForm.find('.type_counts em'),
	regDelEvent	= function(){
		$('.delete a',$commentDiv).attr('onclick','').unbind()
		.click(function(){
			if(!confirm('确定要删除吗？')) return false;
			var $this = $(this);
			$this.hide().after($loading2);
			$.post($this.attr('href'),{mode:'ajax'},function(data){
				if(data == '1') {
					$this.parent().parent().parent().remove();
					var count = parseInt($commentCount.text()) - 1;
					$commentCount.text(count);
				} else {
					$this.show().next().remove();
					alert(data);
				}
			});
			return false;
		});
	},

	//重新注册editbox的事件
	regEvent	= function(){
		regDelEvent();
		$('.editbox').editbox();
		$('.rebox').editbox({type:'re',saveBtnValue:'回 复',afterAppend:regDelEvent});
		$('.quotebox').editbox({type:'quote',saveBtnValue:'回 复',afterAppend:regDelEvent});
		$('.hotrebox').editbox({type:'re',saveBtnValue:'回 复',afterAppend:regDelEvent});
	},

	//ajax翻页
	ajaxTurnPage	= function(data,type){
		if(data){
			var $wallCount = $(".wall_list_count").clone();
			$commentDiv.empty().append(data);
			$commentDiv.prepend($wallCount);
			//$(".all_page").find('a')[0].focus();
		}

		$("#wall_list .all_page>a").each(function(){
			var href=$(this).attr('href');
			var patrn=/page\/(\d+)/;
			var page = patrn.exec(href);
			if(page) {
				href = '/commentlist/turnpage/id/'+object_id+'/page/'+page[1]+'/ajax/1';
				$(this).attr('href','javascript:;').click(function(){
					var location = String(window.location).split("#");
					$("#wall_list .all_page").empty().append($loading2);
					$.post(href,{'type':type},function(data){window.location = location[0]+"#wall_list";ajaxTurnPage(data,type);});
				});
			}
		});
		regEvent();
		regReportEvent();
	};

	ajaxTurnPage('',commentType);
	$textarea.keyup(function(){
		var num = $textarea.val().length;
		$count.html(num.toString());
	});

	$btn_Submit.click(function(){
		var val = $textarea.val();
		var num = val.length;

		if(num<1||/^\s*$/.test(val)) {alert('内容不能为空');return false;}
		if(num>300) {alert('输入内容请限制在300字以内');return false;}

		$btn_Submit.hide().after($loading2);
		if(commentType==23||commentType==25||commentType==24||commentType==30||commentType==12){
			var sendshare = $('#sendshare').val();
			if(sendshare==1) {
				var querydata = {
					relids:relids,
					type:commentType,
					oid:object_id,
					content:val,
					mode:'ajax',
					_xiamitoken:_xiamitoken,
					sendshare:sendshare,
					'content_txt':encodeURIComponent(content_txt),
					'content_html':encodeURIComponent(content_html),
					'topic':encodeURIComponent(topic)
				};
			}else {
				var querydata = {type:commentType,oid:object_id,content:val,relids:relids,mode:'ajax',_xiamitoken:_xiamitoken};
			}
		}else{
			var querydata = {type:commentType,oid:object_id,content:val,relids:relids,mode:'ajax',_xiamitoken:_xiamitoken};	
		}
	
		$.getJSON('/commentlist/add', querydata, function(data){
			$btn_Submit.fadeIn().next().remove();
			if(data.status=='failed') {alert(data.msg);return;}
			$count.html('0');
			if(!topic) {
				$textarea.val('');
				$textarea.next().val('');
			}else {
				$textarea.val(topic) ;
			}
			$commentDiv.find('ul').prepend(data.output);
			regEvent();

			var count = parseInt($commentCount.text()) + 1;
				$commentCount.text(count);
			var $c = $commentDiv.find('ul').children();
			var $last = $($c[0]);
			//$('a',$last).focus();
			//window.scrollTo(0,$(".comments_wall").offset().top);
			$last.hide().fadeIn('fast').animate({backgroundColor:'#FFE222'},'slow').animate({backgroundColor:'#ffffff'},1800);
		});
     
		relids = '';
		return false;
	});
	
	$("textarea[node-type=autoSuggest]").autoSuggest( "/relation/fetch", uid);
});
function detail_report(id,type){
	var url = '/ajax/report/id/'+ encodeURIComponent(id)+'/type/'+encodeURIComponent(type);
	showDialog(url);
}



function ageree(id,state){
	var querydata = {'id':id,'state':state,'user_id':uid,'mode':'ajax', '_xiamitoken':_xiamitoken};
	$.ajax({
		url:'/commentlist/ageree/',
		type: 'GET',
		data:querydata,
		dataType: 'json',
		timeout: 2000,
		beforeSend: function(){
		},
		error: function(data){	
			var url = '/commentlist/ageree/id/'+ encodeURIComponent(id)+'/state/'+encodeURIComponent(state);
			showDialog(url);
		},
		success: function(data){			
			if(data.status==1){
				if(state==1){
					var v = parseInt($('.agereebox_'+id).attr('rel'))+1;
					$('.agereebox_'+id).html('赞('+v+')');
				}else if(state==2){
					var v = parseInt($('.disagereebox_'+id).attr('rel'))+1;	
					$('.disagereebox_'+id).html('弱('+v+')');
				}				
			}
		}
	});	
}

$("#sendshare").click(function() {
    var val = $("#sendshare").val();
    if(val==1) $("#sendshare").val(0);
    if(val==0) $("#sendshare").val(1);   
});

var Renren={
	ApiKey:'8521f0afe9404fafbd73d063dd258df0',
	Receiver:'/renren/xd-receiver',
	CallBack:'/renren/login',
	Init:function(){
		var self=this;
		XN_RequireFeatures(["Connect"], function(){
			XN.Main.init(self.ApiKey,self.Receiver,{"doNotUseCachedConnectState":true,'permsToRequestOnConnect':'publish_feed'});
		});
	},
	
	Login:function(){
		XN.Connect.requireSession(function(){
			window.location='/renren/login';
		});
		setTimeout(function(){
			$('#shadow').remove();
			}, 300);
	},
	
	Logout:function(){
		var self=this;
		XN_RequireFeatures(["Connect"], function(){
			XN.Main.init(self.ApiKey,self.Receiver);
			XN.Connect.get_status().waitUntilReady(function (login_state) {
				if (login_state != XN.ConnectState.connected) self.Init();
				else {
					var $msg = $('#logoutMsg').find('ul');
					$msg.append('<li style="display:none">正在等待人人连接的退出! 请稍侯...</li>');
					var $last = $msg.find('li:last');
					$last.fadeIn();
					var timer = setInterval(function(){$last.html($last.html()+'.');},500);
					XN.Connect.logout(function () {
						clearInterval(timer);
						$last.html('人人连接已成功退出！');
						setTimeout(function(){$last.fadeOut();},3000);
						self.Init();
					});
				}
			});
		});
	}
};

