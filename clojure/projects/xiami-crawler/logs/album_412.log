var BackTop=function(btnId){
	var btn=document.getElementById(btnId);
	var browserName=navigator.userAgent.toLowerCase();
	if(/chrome/i.test(browserName) && /webkit/i.test(browserName) && /mozilla/i.test(browserName)){
