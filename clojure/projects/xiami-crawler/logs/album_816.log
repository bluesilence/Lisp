	showDialog(url);
}
$(document).ready(function() {
	$('#cover_lightbox').lightBox({
		imageLoading:'http://img.xiami.net/res/js/jquery/jquery_lightbox_images/lightbox-ico-loading.gif',
		imageBtnPrev:'http://img.xiami.net/res/js/jquery/jquery_lightbox_images/lightbox-btn-prev.gif',	
		imageBtnNext:'http://img.xiami.net/res/js/jquery/jquery_lightbox_images/lightbox-btn-next.gif',
		imageBtnClose:'http://img.xiami.net/res/js/jquery/jquery_lightbox_images/lightbox-btn-close_tiny.gif',
		imageBlank:'http://img.xiami.net/res/js/jquery/jquery_lightbox_images/lightbox-blank.gif'
	});
	var delGrade = $(".delGrade");
	$(".grade").hover(function(){
		delGrade.show();
	},function(){
		delGrade.hide();
	});
	delGrade.bind('click',function(){
		var $this = $(this);
		$this.siblings('a').removeClass('selected');
		$.getJSON('/album/review',{grade:0,id:aid,submit:1,mod:'ajax',_xiamitoken:_xiamitoken},function(data){
			if(data.status=='failed'){alert(data.msg);return;}
			$this.siblings('input').val('0');
			window.location.reload();
		});
	});
	/**/
	var target='.grade a';
	//var i=parseInt($('#album_cover .grade_v').val());
	var i = '';
	if(parseInt($('#album_cover .grade_v').val())){
		for (j=0; j<i; j++){$($(target).get(j)).addClass('selected');}
		$('.grade span').text($($(target).get(i-1)).text());
	}
	$(target).mouseover(function(){
		$(target).removeClass('hover').removeClass('selected');
		var i=parseInt($(target).index($(this)[0])); 
		for (j=0; j<=i; j++){$($(target).get(j)).addClass('hover');}
		$('.grade span').text($(this).text()); 
	}).click(function(){
		$(target).removeClass('hover').removeClass('selected');
		var i=parseInt($(target).index($(this)[0])); 
		for (j=0; j<=i; j++){$($(target).get(j)).addClass('selected');}
		$('#album_cover .grade_v').val(i+1);
		//$('.grade span').text($(this).text());
	}).mouseout(function(){
		$(target).removeClass('hover').removeClass('selected');
		var i=parseInt($('#album_cover .grade_v').val());		
		for (j=0; j<i; j++){$($(target).get(j)).addClass('selected');}
		if(!parseInt($('#album_cover .grade_v').val())){
		$('.grade span').text('');
		}else{
		$('.grade span').text($($(target).get(i-1)).text());
		}
	});
	/**/

	$('#album_acts').mouseenter(function(){
		$(this).addClass('acts_hover');
		$('.do_fav').mouseenter(function(){
			$(this).find('ul').show();
		})
		.mouseleave(function(){
			$(this).find('ul').hide();
		});
	})
	.mouseleave(function(){
		$(this).removeClass('acts_hover');
	});

	//专辑试听数
	var playCount = 0;
	var temp = $('.song_hot');
	for(var i=0;i<temp.length;i++){
		playCount += $(temp[i]).text()-0;
	}
	$('#play_count_num').html(playCount);
	
	$(".album_intro_toggle a").toggle(
		function() {
			$("#album_intro").removeClass("intro_less");
			$(this).attr('id','show_less').html("收缩");
		},
		function() {
			$("#album_intro").addClass("intro_less");
			$(this).attr('id','show_more').html("详细");
		}
	);
});
	//提交音频
	$('#submit_music').hover(function(){
		$(this).find('.show_tips').show();
	},function(){
		$(this).find('.show_tips').hide();
	});
$('#more_btn').bind("click", function(){
	if (!$(this).hasClass("song_less")) {
		$(this).html('- 更少').addClass("song_less");
		$('.share_btns').hide();
		$('#full_share').show();
	} else {
		$(this).html('+ 更多').removeClass("song_less");
		$('#full_share').hide();
		$('.share_btns').show();
	}
});
)} 


